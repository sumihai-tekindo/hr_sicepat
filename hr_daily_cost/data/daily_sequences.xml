<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <!-- Sequences for sale.order -->
        <record id="seq_type_daily" model="ir.sequence.type">
            <field name="name">Daily Cost</field>
            <field name="code">daily.cost</field>
        </record>

        <record id="seq_daily_cost" model="ir.sequence">
            <field name="name">Daily Cost</field>
            <field name="code">daily.cost</field>
            <field name="prefix">DAILY%(range_year)s%(range_month)s%(range_day)s</field>
            <field name="padding">3</field>
            <field name="implementation">no_gap</field>
            <field name="use_date_range" eval="True"/>
            <field name="date_range_type">daily</field>
        </record>

    </data>
</openerp>
